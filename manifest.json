{
  "manifest_version": 3,
  "name": "viewbg",
  "version": "0",
  "description": "View background image",
  "icons": {
    "128": "128.png"
  },
  "permissions":[
    "contextMenus",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background":{
    "service_worker": "bg.js"
  }
}